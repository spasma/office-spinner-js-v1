{
  "name": "Kantoor Roulette",
  "version": "0.9.4",
  "manifest_version": 2,
  "description": "OfficiÃ«le Kantoor Roulette Chrome Plugin",
  "homepage_url": "http://kantoorroulette.nl",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "nl",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Koffie!",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "contextMenus",
    "cookies",
    "*://*.kantoorroulette.nl/*",
    "*://kantoorroulette.nl/*",
    "tts",
    "ttsEngine",
    "storage",
    "idle",
    "notifications",
    "geolocation"
  ]
}